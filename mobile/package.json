{
  "name": "sillytavern-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "android:devclient": "cd android; ./gradlew assembleDevDebug",
    "android:devclient:win": "cd android && .\\gradlew.bat assembleDevDebug",
    "android:prod": "cd android; ./gradlew assembleProdRelease",
    "android:prod:win": "cd android && .\\gradlew.bat assembleProdRelease",
    "android:install:devclient": "adb install -r android/app/build/outputs/apk/devclient/debug/app-devclient-debug.apk",
    "android:install:prod": "adb install -r android/app/build/outputs/apk/prod/release/app-prod-release.apk",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test:serde": "tsx scripts/test-serialization.ts",
    "test:importer": "tsx scripts/test-role-importer.ts",
    "fetch:card": "tsx scripts/fetch-janny-card.ts",
    "i": "npm install --legacy-peer-deps",
    "ad": "npm run android:devclient",
    "adw": "npm run android:devclient:win",
    "ap": "npm run android:prod",
    "apw": "npm run android:prod:win",
    "aid": "npm run android:install:devclient",
    "aip": "npm run android:install:prod",
    "id": "npm run ios:devclient",
    "ip": "npm run ios:prod",
    "sc": "expo start --dev-client -c"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/drawer": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "buffer": "^6.0.3",
    "expo": "~54.0.8",
    "expo-clipboard": "~8.0.7",
    "expo-crypto": "~15.0.7",
    "expo-file-system": "~19.0.14",
    "expo-linking": "^8.0.8",
    "expo-router": "~6.0.6",
    "expo-secure-store": "~15.0.7",
    "immer": "^10.1.1",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@react-native/eslint-config": "0.73.2",
    "@types/react": "~19.1.10",
    "@types/react-native": "0.73.0",
    "babel-preset-expo": "~54.0.0",
    "eslint": "8.57.0",
    "expo-build-properties": "^1.0.8",
    "prettier": "^2.8.8",
    "react-native-worklets": "^0.5.1",
    "react-refresh": "^0.14.0",
    "tsx": "^4.19.2",
    "typescript": "~5.9.2"
  }
}
